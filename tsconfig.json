{
    "compilerOptions": {
        "target": "es2019",
        "module": "esnext",
        "jsx": "react",
        "sourceMap": true ,
        "strict": false,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true
    },
    "exclude": [
        "**/__tests__/**",
        "**/dist*/**"
    ]
}
